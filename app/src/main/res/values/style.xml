<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TitleLabelTitleStyle">
        <item name="fontWeight">600</item>
        <item name="android:textSize">30sp</item>
        <item name="fontFamily">@font/poppins_bold</item>
    </style>
    <style name="LabelTitleStyle">
        <item name="fontWeight">500</item>
        <item name="android:textSize">18sp</item>
        <item name="fontFamily">@font/poppins_semi_bold</item>
        <item name="android:textColor">@color/label_txt_color</item>
    </style>
    <style name="DialogTitleLabel">
        <item name="fontWeight">500</item>
        <item name="android:textSize">18sp</item>
        <item name="fontFamily">@font/poppins_semi_bold</item>
        <item name="android:textColor">@color/txt_green</item>
    </style>

    <style name="ButtonStyle">
        <item name="cornerRadius">8dp</item>
        <item name="fontFamily">@font/poppins_semi_bold</item>
        <item name="android:textSize">18sp</item>
        <item name="rippleColor">@null</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>